<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AutoLottoMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AutoLottoMaui"
    Shell.FlyoutBehavior="Disabled"
    Title="AutoLottoMaui">

    <TabBar>

        <Tab Title="My Tickets">
           <ShellContent ContentTemplate="{DataTemplate local:MyTickets}" Route="MyTickets"/>
    </Tab>

        <Tab Title="Powerball">
           <ShellContent ContentTemplate="{DataTemplate local:PowerBallPage}" Route="PowerBallPage"/>
    </Tab>
    
    <Tab Title="Mega Millions">
           <ShellContent ContentTemplate="{DataTemplate local:MegaMillionsPage}" Route="MegaMillionsPage"/>
    </Tab>


    </TabBar>
</Shell>
